<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" fx:id="AnchorPane" prefHeight="365.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="umja.FXMLDocumentController">
    <TextArea fx:id="ta_Output" editable="false" layoutX="14.0" layoutY="151.0" prefHeight="200.0"
              prefWidth="572.0"/>
    <Label layoutX="14.0" layoutY="116.0" prefHeight="33.0" prefWidth="157.0" text="Console Output:"
           textAlignment="CENTER" textOverrun="CLIP">
        <font>
            <Font size="20.0"/>
        </font>
    </Label>
    <Button fx:id="btn_loadUMLFile" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#openUMLFile"
            prefHeight="88.0" prefWidth="278.0" text="Import UML ">
        <font>
            <Font size="33.0"/>
        </font>
    </Button>
    <Button fx:id="btn_Convert" layoutX="308.0" layoutY="14.0" mnemonicParsing="false" onAction="#convertToJava"
            prefHeight="88.0" prefWidth="278.0" text="Convert">
        <font>
            <Font size="33.0"/>
        </font>
    </Button>
</AnchorPane>
